[alias]
	br = branch
	ci = commit
	cia = commit --amend
	co = checkout
	cp = cherry-pick
	df = diff
	dfc = diff --cached
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen (%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	lgall = log --color --graph --all --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen (%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	lgd = log --color --graph --decorate --oneline
	lgda = log --color --graph --decorate --oneline --all
	pom = push origin master
	sh = show
	shs = show --name-status
	st = status
